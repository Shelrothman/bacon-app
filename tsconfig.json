{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "jsx": "react-jsx",
        "strict": true,
        "resolveJsonModule": true,
        "moduleResolution": "Node",
        "target": "ES2020",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "lib": [
            "dom.iterable",
            "ES6",
            "DOM",
            "ESNext"
        ]
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}